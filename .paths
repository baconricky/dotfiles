# Add `~/bin` to the `$PATH`

PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"

export HOMEBREW_PATH="$(brew --prefix homebrew/php/php56)/bin"
export MY_BIN="$HOME/sbin:$HOME/bin"
export LOCAL_BIN="/usr/local/sbin:/usr/local/bin"
export RUBY_GEM_PATH="$HOME/.rvm/gems/ruby-2.1.2/bin"
export RUBY_GEM_GLOBAL_PATH="$HOME/.rvm/gems/ruby-2.1.2@global/bin"
export RUBIES_PATH="$HOME/.rvm/rubies/ruby-2.1.2/bin"
export PHP_PATH="/usr/local/Cellar/php56/5.6.0-rc.4/bin"
export RVM_USER_PATH="$HOME/.rvm/bin"
export RVM_LOCAL_PATH="/usr/local/mysql/bin"
export PYTHONPATH="$HOME/lib/python"
export LDFLAGS="-L/usr/local/opt/openssl/lib"
export CPPFLAGS="-I/usr/local/opt/openssl/include"
export GNUBIN="$(brew --prefix coreutils)/libexec/gnubin"

export PATH="$GNUBIN:$HOMEBREW_PATH:$MY_BIN:$LOCAL_BIN:$RUBY_GEM_PATH:$RUBY_GEM_GLOBAL_PATH:$RUBIES_PATH:$PHP_PATH:$RVM_USER_PATH:$RVM_LOCAL_PATH:$PYTHONPATH:$LDFLAGS:$CPPFLAGS:$PATH"

